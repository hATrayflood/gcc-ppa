#! /bin/sh

prctl=

case "$(prctl --unaligned=)" in *signal)
    echo >&2 "$(basename $0): ignore unaligned memory accesses"
    prctl="prctl --unaligned=default"
esac

exec $prctl /usr/bin/gij-4.4.bin "$@"
